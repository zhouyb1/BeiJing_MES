@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Index.cshtml";
}
<style>
    .time {
        background: url(/Content/images/datepicker_icon.png) right 0 no-repeat #FFF;
    }
</style>
<div class="am-layout">
    <div class="am-layout-center">
        <div class="am-layout-wrap am-layout-wrap-notitle">
            <div class="am-layout-tool">
                <div class="am-layout-tool-left">
                    <div class="am-layout-tool-item">
                        <div id="datesearch"></div>
                    </div>
                    <div class="am-layout-tool-item">
                        <div id="multiple_condition_query">
                            <div class="am-query-formcontent">
                                @*<div class="col-xs-12 am-form-item">
                                    <div class="am-form-item-title">起始日期</div>
                                    <input id="StartTime" type="text" class="form-control am-input-wdatepicker" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd' })" value="@DateTime.Now.AddMonths(-1).ToString("yyyy-MM-dd")" isvalid="yes" checkexpession="NotNull" />
                                </div>

                                <div class="col-xs-12 am-form-item">
                                    <div class="am-form-item-title">截至日期</div>
                                    <input id="EndTime" type="text" class="form-control am-input-wdatepicker" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd' })" value="@DateTime.Now.ToString("yyyy-MM-dd")" isvalid="yes" checkexpession="NotNull" />
                                </div>*@
                                
                                <div class="col-xs-6 am-form-item">
                                    <div class="am-form-item-title">物料名称</div>
                                    <div id="GoodsCode" class="form-control"></div>
                                </div>
                                
                                <div class="col-xs-6 am-form-item">
                                    <div class="am-form-item-title">仓库名称</div>
                                    <div id="StockCode" class="form-control"></div>
                                </div>
                                <div class="col-xs-12 am-form-item">
                                    <div class="am-form-item-title">领料日期</div>
                                    <div id="area" class="col-xs-5 am-form-area">
                                        <input id="StartTime" type="text" class="form-control time" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd 00:00:00' })" value="@DateTime.Now.AddDays(-6).ToString("yyyy-MM-dd 00:00:00")" isvalid="yes" checkexpession="NotNull" />
                                    </div>
                                    <div class="am-form-item-title" style="margin-left: 45%">至</div>
                                    <div class="col-xs-5" style="margin-left: 16%">
                                        <input id="EndTime" type="text" class="form-control time" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd 23:59:59' })" value="@DateTime.Now.ToString("yyyy-MM-dd 23:59:59")" isvalid="yes" checkexpession="NotNull" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="am-layout-tool-right">
                    <div class=" btn-group btn-group-sm">
                        <a id="am_refresh" class="btn btn-default"><i class="fa fa-refresh"></i></a>
                        <a id="am_export" class="btn btn-default"><i class="fa fa-sign-out"></i>&nbsp;导出Excel</a>
                        <a id="am_print" class="btn btn-default"><i class="fa fa-print"></i>&nbsp;打印 </a>
                    </div>
                </div>
            </div>
            <div class="am-layout-body" id="girdtable"></div>
        </div>
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/PickingMater/CollarRport.js")
