@{
    ViewBag.Title = "线边仓退料到仓库";
    Layout = "~/Views/Shared/_GirdForm.cshtml";
}
<div class="am-form-wrap" id="form">
    
    <div class="col-xs-4 am-form-item"  data-table="Mes_BackStockHead" >
        <div class="am-form-item-title">退仓库单号<font face="宋体">*</font></div>
        <input id="B_BackStockNo" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" value="@ViewBag.OrderNo" disabled="disabled" style="background: #f1efef" />
    </div>
    <div class="col-xs-4 am-form-item"  data-table="Mes_BackStockHead" >
        <div class="am-form-item-title">仓库名称<font face="宋体">*</font></div>
        <div id="B_StockName" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-4 am-form-item" data-table="Mes_BackStockHead">
        <div class="am-form-item-title">仓库编码<font face="宋体">*</font></div>
        <input id="B_StockCode" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" style="background: #f1efef" disabled="disabled" />
    </div>
    <div class="col-xs-4 am-form-item"  data-table="Mes_BackStockHead" >
        <div class="am-form-item-title">退库仓库编码<font face="宋体">*</font></div>
        <input id="B_StockToCode" type="text" class="form-control" style="background: #f1efef" disabled="disabled" />
    </div>
    <div class="col-xs-4 am-form-item"  data-table="Mes_BackStockHead" >
        <div class="am-form-item-title">退库仓库名称<font face="宋体">*</font></div>
        <div id="B_StockToName" type="text" class="form-control"></div>
    </div>
    <div class="col-xs-4 am-form-item" data-table="Mes_BackStockHead">
        <div class="am-form-item-title">订单时间<font face="宋体">*</font></div>
        <input id="B_OrderDate" type="text" class="form-control" class="form-control am-input-wdatepicker" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd HH:mm:ss' })" value="@DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss")" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-4 am-form-item"  data-table="Mes_BackStockHead" >
        <div class="am-form-item-title">备注</div>
        <input id="B_Remark" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item" data-table="Mes_RequistHead">
        <div class="am-form-item-title"></div>
        <a id="am_add" class="btn btn-default"><i class="fa fa-plus"></i>&nbsp;添加商品</a>
    </div>
    <div class="col-xs-12 am-form-item am-form-item-grid">
        <div id="Mes_BackStockDetail"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/BackStockManager/Form.js")
