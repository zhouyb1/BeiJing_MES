@{
    ViewBag.Title = "其它出库单";
    Layout = "~/Views/Shared/_GirdForm.cshtml";
}
<div class="am-form-wrap" id="form">
    <div class="col-xs-6 am-form-item" data-table="Mes_OtherOutHead">
        <div class="am-form-item-title">单号</div>
        <input id="O_OtherOutNo" type="text" class="form-control" disabled="disabled" placeholder="系统自动生成" />
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_OtherOutHead" style="display:none">
        <div class="am-form-item-title">单据状态<font face="宋体">*</font></div>
        <div id="O_Status" type="text" class="form-control" disabled="disabled"></div>
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_OtherOutHead">
        <div class="am-form-item-title">仓库名称<font face="宋体">*</font></div>
        <div id="O_StockName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull">  </div>
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_OtherOutHead">
        <div class="am-form-item-title">仓库编码</font></div>
        <input id="O_StockCode" type="text" class="form-control"  disabled="disabled" style="background: #f1efef" />
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_OtherOutHead">
        <div class="am-form-item-title">部门名称<font face="宋体">*</font></div>
        <div id="O_DepartName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_OtherOutHead">
        <div class="am-form-item-title">部门编码</div>
        <input id="O_DepartCode" type="text" class="form-control"  disabled="disabled" style="background: #f1efef" />
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_OtherOutHead">
        <div class="am-form-item-title">备注</div>
        <input id="O_Remark" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 am-form-item" style="display:none" data-table="Mes_OtherOutHead">
        <div class="am-form-item-title">月结</div>
        <input id="MonthBalance" type="text" class="form-control"/>
    </div>
    <div class="col-xs-12 am-form-item" data-table="Mes_ProOutHead">
        <div class="am-form-item-title"></div>
        <a id="am_add" class="btn btn-default"><i class="fa fa-plus"></i>&nbsp;添加商品</a>
    </div>
    <div class="col-xs-12 am-form-item am-form-item-grid">
        <div id="Mes_OtherOutDetail"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/Mes_OtherOutHead/Form.js")
