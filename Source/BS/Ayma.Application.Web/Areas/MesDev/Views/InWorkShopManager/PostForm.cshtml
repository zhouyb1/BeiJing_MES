@{
    ViewBag.Title = "车间入库到线边仓详情";
    Layout = "~/Views/Shared/_GirdForm.cshtml";
}
<div class="am-form-wrap" id="form">

    <div class="col-xs-6 am-form-item" data-table="Mes_InWorkShopHead">
        <div class="am-form-item-title">入库单号</div>
        <input id="I_InNo" type="text" class="form-control" value="@ViewBag.InNo" disabled="disabled" style="background: #f1efef" />
    </div>

    <div class="col-xs-6 am-form-item" data-table="Mes_InWorkShopHead">
        <div class="am-form-item-title">仓库</div>
        <div id="I_StockName" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_InWorkShopHead">
        <div class="am-form-item-title">仓库编码</div>
        <input id="I_StockCode" type="text" class="form-control" disabled="disabled" style="background: #f1efef" />
    </div>

    <div class="col-xs-6 am-form-item" data-table="Mes_InWorkShopHead">
        <div class="am-form-item-title">生产订单号<font face="宋体">*</font></div>
        <input id="I_OrderNo" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
       
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_InWorkShopHead">
        <div class="am-form-item-title">生产订单时间<font face="宋体">*</font></div>
        <input id="I_OrderDate" type="text" class="form-control am-input-wdatepicker" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd HH:mm:ss' })" value="@DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss")" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_InWorkShopHead">
        <div class="am-form-item-title">备注</div>
        <input id="I_Remark" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_InWorkShopHead">
        <div class="am-form-item-title">调拨车间</div>
        <div id="I_WorkShop" type="text" class="form-control" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-12 am-form-item am-form-item-grid">
        <div id="Mes_InWorkShopDetail"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/InWorkShopManager/PostForm.js")


