@{
    ViewBag.Title = "工序表单";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="am-form-wrap" id="form">
    <div>
        <input id="P_RecordCode" hidden="true" />
    </div>
    <div class="col-xs-12 am-form-item" data-table="Mes_Proce">
        <div class="am-form-item-title">工序号<font face="宋体">*</font>
        </div>
        <input id="P_ProNo" type="text" class="form-control" isvalid="yes" checkexpession="NotNull"/>
    </div>

    <div class="col-xs-12 am-form-item" data-table="Mes_Proce">
        <div class="am-form-item-title">工序名称<font face="宋体">*</font>
        </div>
        <input id="P_ProName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull"/>
    </div>
    <div class="col-xs-12 am-form-item" data-table="Mes_Proce">
        <div class="am-form-item-title">车间<font face="宋体">*</font>
        </div>
        <div id="P_WorkShop"></div>
    </div>

    <div class="col-xs-12 am-form-item" data-table="Mes_Proce">
        <div class="am-form-item-title">是否最后工序<font face="宋体">*</font>
        </div>
        <div id="P_Kind" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-12 am-form-item" data-table="Mes_Proce">
        <div class="am-form-item-title">备注
        </div>
        <input id="P_Remark" type="text" class="form-control"/>
    </div>

</div>
@Html.AppendJsFile("/Areas/MesDev/Views/ProceManager/ProceForm.js")
