@{
    ViewBag.Title = "温湿度采集参数设置";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="am-form-wrap" id="form">
    <div class="col-xs-12 am-form-item"  data-table="Mes_Humiture" >
        <div class="am-form-item-title">温湿度类型<font face="宋体">*</font></div>
        <div id="H_Kind" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-12 am-form-item"  data-table="Mes_Humiture" >
        <div class="am-form-item-title">IP<font face="宋体">*</font></div>
        <input id="H_IP" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="Mes_Humiture" >
        <div class="am-form-item-title">端口<font face="宋体">*</font></div>
        <input id="H_Port" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="Mes_Humiture" >
        <div class="am-form-item-title">上限值<font face="宋体">*</font></div>
        <input id="H_Up" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="Mes_Humiture" >
        <div class="am-form-item-title">下限值<font face="宋体">*</font></div>
        <input id="H_Down" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="Mes_Humiture" >
        <div class="am-form-item-title">地址</div>
        <input id="H_Address" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="Mes_Humiture" >
        <div class="am-form-item-title">备注</div>
        <input id="H_Remark" type="text" class="form-control"  />
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/HumitureManager/Form.js")
