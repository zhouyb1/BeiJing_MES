@{
    ViewBag.Title = "消耗物料单据制作";
    Layout = "~/Views/Shared/_GirdForm.cshtml";
}
<div class="am-form-wrap" id="form">
    
    <div class="col-xs-6 am-form-item"  data-table="Mes_ExpendHead" >
        <div class="am-form-item-title">单据编号</div>
        <input id="E_ExpendNo" type="text" class="form-control" placeholder="系统自动生成" disabled="disabled"/>
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_ExpendHead" >
        <div class="am-form-item-title">仓库</div>
        <div id="E_StockCode" type="text" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_ExpendHead">
        <div class="am-form-item-title">单据日期</div>
        <input id="E_OrderDate" type="text" class="form-control am-input-wdatepicker" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd' })" value="@DateTime.Now.ToString("yyyy-MM-dd")" isvalid="yes" checkexpession="NotNull" disabled="@ViewBag.disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_ExpendHead" >
        <div class="am-form-item-title">备注</div>
        <input id="E_Remark" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 am-form-item" data-table="Mes_CollarHead">
        <div class="am-form-item-title"></div>
        <a id="am_add" class="btn btn-default"><i class="fa fa-plus"></i>&nbsp;添加物料</a>
    </div>
    <div class="col-xs-12 am-form-item am-form-item-grid">
        <div id="Mes_ExpendDetail"></div>
    </div>
    
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/Mes_ExpendManager/Form.js")
