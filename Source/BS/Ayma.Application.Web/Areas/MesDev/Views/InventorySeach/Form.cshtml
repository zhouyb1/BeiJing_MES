@{
    ViewBag.Title = "库存查询";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="am-form-wrap" id="form">
    <div class="col-xs-6 am-form-item"  data-table="Mes_Inventory" >
        <div class="am-form-item-title">仓库编码</div>
        <input id="I_StockCode" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_Inventory" >
        <div class="am-form-item-title">仓库名称</div>
        <input id="I_StockName" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_Inventory" >
        <div class="am-form-item-title">商品编码</div>
        <input id="I_GoodsCode" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_Inventory" >
        <div class="am-form-item-title">商品名称</div>
        <input id="I_GoodsName" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_Inventory" >
        <div class="am-form-item-title">单位</div>
        <input id="I_Unit" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_Inventory" >
        <div class="am-form-item-title">数量</div>
        <input id="I_Qty" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_Inventory" >
        <div class="am-form-item-title">批次</div>
        <input id="I_Batch" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_Inventory" >
        <div class="am-form-item-title">备注</div>
        <input id="I_Remark" type="text" class="form-control" disabled="disabled" />
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/InventorySeach/Form.js")
