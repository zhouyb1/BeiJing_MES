@{
    ViewBag.Title = "成品出库单制作";
    Layout = "~/Views/Shared/_GirdForm.cshtml";
}
<style>
    .jfgrid-data-cell {
        border-bottom: 1px solid #ddd;
        border-right: 1px solid #ddd;
    }
</style>
<div class="am-form-wrap" id="form">
    <div class="col-xs-6 am-form-item" data-table="Mes_ProOutHead">
        <div class="am-form-item-title">出库单号</div>
        <input id="P_ProOutNo" type="text" class="form-control" value="@ViewBag.OutNo" disabled="disabled" style="background: #f1efef" />
    </div>

    <div class="col-xs-6 am-form-item" data-table="Mes_ProOutHead">
        <div class="am-form-item-title">仓库<font face="宋体">*</font></div>
        <div id="P_StockName" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_ProOutHead">
        <div class="am-form-item-title">仓库编码</div>
        <input id="P_StockCode" type="text" class="form-control" disabled="disabled" style="background: #f1efef" />
    </div>

    <div class="col-xs-6 am-form-item" data-table="Mes_ProOutHead">
        <div class="am-form-item-title">生产订单号<font face="宋体">*</font></div>
        <div id="P_OrderNo" class="form-control" isvalid="yes" checkexpession="NotNull"></div>

    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_ProOutHead">
        <div class="am-form-item-title">生产订单时间<font face="宋体">*</font></div>
        <input id="P_OrderDate" type="text" class="form-control am-input-wdatepicker" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd HH:mm:ss' })" value="@DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss")" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_ProOutHead">
        <div class="am-form-item-title">备注</div>
        <input id="P_Remark" type="text" class="form-control" />
    </div>
    <div class="col-xs-12 am-form-item" data-table="Mes_ProOutHead">
        <div class="am-form-item-title"></div>
        <a id="am_add" class="btn btn-default"><i class="fa fa-plus"></i>&nbsp;添加商品</a>
    </div>
    <div class="col-xs-12 am-form-item am-form-item-grid" >
        <div id="Mes_ProOutDetail"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/ProOutMake/Form.js")
