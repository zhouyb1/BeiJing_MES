@{
    ViewBag.Title = "成品出库单制作";
    Layout = "~/Views/Shared/_GirdForm.cshtml";
}
<style>
    .jfgrid-data-cell {
        border-bottom: 1px solid #ddd;
        border-right: 1px solid #ddd;
    }
</style>
<div class="am-form-wrap" id="form">
    @*<div class="col-xs-6 am-form-item"  data-table="Mes_ProOutHead" >
        <div class="am-form-item-title">出库单号</div>
        <input id="P_ProOutNo" type="text" class="form-control"  />
    </div>*@
    <div class="col-xs-6 am-form-item"  data-table="Mes_ProOutHead" >
        <div class="am-form-item-title">仓库编码<font face="宋体">*</font></div>
        <div id="P_StockCode" class="form-control" isvalid="yes" checkexpession="NotNull" disabled="disabled"></div>
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_ProOutHead" >
        <div class="am-form-item-title">仓库名称</div>
        <input id="P_StockName" type="text" class="form-control" disabled="disabled" style="background: #f1efef" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_ProOutHead" >
        <div class="am-form-item-title">生产订单号</div>
        <input id="P_OrderNo" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_ProOutHead" >
        <div class="am-form-item-title">订单时间<font face="宋体">*</font></div>
        <input id="P_OrderDate" type="text" class="form-control am-input-wdatepicker" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd HH:mm:ss' })" value="@DateTime.Now.ToString("yyyy-MM-dd HH:mm:ss")" isvalid="yes" checkexpession="NotNull" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_ProOutHead" >
        <div class="am-form-item-title">出库单状态<font face="宋体">*</font></div>
        <div id="P_Status" isvalid="yes" checkexpession="NotNull" disabled="disabled"></div>
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_ProOutHead">
        <div class="am-form-item-title">备注</div>
        <input id="P_Remark" type="text" class="form-control" disabled="disabled" />
    </div>
   
    <div class="col-xs-12 am-form-item am-form-item-grid" >
        <div id="Mes_ProOutDetail"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/ProOutMake/SearchForm.js")
