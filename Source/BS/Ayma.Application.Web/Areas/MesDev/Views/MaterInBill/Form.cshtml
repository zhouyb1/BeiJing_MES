@{
    ViewBag.Title = "入库单制作";
    Layout = "~/Views/Shared/_GirdForm.cshtml";
}
<style>
    .jfgrid-data-cell {
        border-bottom: 1px solid #ddd;
        border-right: 1px solid #ddd;
    }
</style>
<div class="am-form-wrap" id="form">
    <div class="col-xs-6 am-form-item"  data-table="Mes_MaterInHead" >
        <div class="am-form-item-title">入库单号</div>
        <input id="M_MaterInNo" type="text" class="form-control" disabled="disabled"  />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_MaterInHead" >
        <div class="am-form-item-title">物料编码</div>
        <input id="M_StockCode" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_MaterInHead" >
        <div class="am-form-item-title">物料名称</div>
        <input id="M_StockName" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_MaterInHead" >
        <div class="am-form-item-title">生产订单号</div>
        <input id="M_OrderNo" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_MaterInHead" >
        <div class="am-form-item-title">订单时间</div>
        <input id="M_OrderDate" type="text" class="form-control" disabled="disabled" />
    </div>
    @*<div class="col-xs-6 am-form-item"  data-table="Mes_MaterInHead" >
        <div class="am-form-item-title">状态</div>
        <input id="M_Status" type="text" class="form-control"  />
    </div>*@
    <div class="col-xs-6 am-form-item"  data-table="Mes_MaterInHead" >
        <div class="am-form-item-title">备注</div>
        <input id="M_Remark" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-12 am-form-item am-form-item-grid" >
        <div id="Mes_MaterInDetail"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/MaterInBill/Form.js")
