@{
    ViewBag.Title = "商品二级分类";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="am-form-wrap" id="form">
   
    <div class="col-xs-12 am-form-item"  data-table="Mes_GoodKind" >
        <div class="am-form-item-title">编码</div>
        <input id="G_Code" type="text" class="form-control" placeholder="编号自动生成" disabled="disabled" />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="Mes_GoodKind" >
        <div class="am-form-item-title">名称<font face="宋体">*</font></div>
        <input id="G_Name" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 am-form-item"  data-table="Mes_GoodKind" >
        <div class="am-form-item-title">备注</div>
        <input id="G_Remark" type="text" class="form-control"  />
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/GoodKind/Form.js")
