@{
    ViewBag.Title = "物料转换对应表";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="am-form-wrap" id="form">
    <div class="col-xs-6 am-form-item"  data-table="Mes_Convert" >
        <div class="am-form-item-title">物料编码<font face="宋体">*</font></div>
        <div id="C_Code" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_Convert" >
        <div class="am-form-item-title">物料名称</div>
        <input id="C_Name" type="text" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_Convert" >
        <div class="am-form-item-title">转换物料编码<font face="宋体">*</font></div>
        <div id="C_SecCode" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_Convert" >
        <div class="am-form-item-title">转换物料名称</div>
        <input id="C_SecName" type="text" class="form-control" disabled="disabled" />
    </div>
    
    <div class="col-xs-6 am-form-item"  data-table="Mes_Convert" >
        <div class="am-form-item-title">备注</div>
        <input id="C_Remark" type="text" class="form-control"  />
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/Convert/Form.js")
