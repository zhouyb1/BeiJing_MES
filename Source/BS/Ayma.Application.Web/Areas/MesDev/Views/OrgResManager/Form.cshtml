@{
    ViewBag.Title = "组装与拆分单据制作";
    Layout = "~/Views/Shared/_GirdForm.cshtml";
}
<div class="am-form-wrap" id="form">
   
    <div class="col-xs-4 am-form-item"  data-table="Mes_OrgResHead" >
        <div class="am-form-item-title">单据号码</div>
        <input id="O_OrgResNo" type="text" class="form-control" placeholder="系统自动生成" class="form-control" disabled="disabled" />
    </div>
    <div class="col-xs-4 am-form-item"  data-table="Mes_OrgResHead" >
        <div class="am-form-item-title">工序<font face="宋体">*</font></div>
        <div id="O_ProCode" type="text" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-4 am-form-item" data-table="Mes_OrgResHead">
        <div class="am-form-item-title">日耗仓名称<font face="宋体">*</font></div>
        <div id="O_StockName" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-4 am-form-item" data-table="Mes_OrgResHead">
        <div class="am-form-item-title">日耗仓编码</div>
        <input id="O_StockCode" type="text" class="form-control" readonly="readonly" />
    </div>
    <div class="col-xs-4 am-form-item" data-table="Mes_OrgResHead">
        <div class="am-form-item-title">班组<font face="宋体">*</font></div>
        <div id="O_TeamCode" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-4 am-form-item"  data-table="Mes_OrgResHead" >
        <div class="am-form-item-title">备注</div>
        <input id="O_Remark" type="text" class="form-control"  />
    </div>
    @*<div class="col-xs-12 am-form-item" data-table="Mes_RequistHead">
        <div class="am-form-item-title"></div>
        <a id="am_add" class="btn btn-default"><i class="fa fa-plus"></i>&nbsp;添加物料</a>
    </div>*@

    <div class="col-xs-12 am-form-item am-form-item-grid">
        <div id="Mes_OrgResDetail_h"></div>
    </div>

    <div class="col-xs-12 am-form-item am-form-item-grid">
        <div id="Mes_OrgResDetail_d"></div>
    </div>
</div>
