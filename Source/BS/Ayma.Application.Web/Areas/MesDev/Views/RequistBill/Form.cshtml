@{
    ViewBag.Title = "调拨单制作";
    Layout = "~/Views/Shared/_GirdForm.cshtml";
}
<style>
    .jfgrid-data-cell {
        border-bottom: 1px solid #ddd;
        border-right: 1px solid #ddd;
    }
</style>
<div class="am-form-wrap" id="form">
    <div class="col-xs-6 am-form-item" data-table="Mes_RequistHead">
        <div class="am-form-item-title">单据号码</div>
        <input id="R_RequistNo" type="text" class="form-control" disabled="disabled" placeholder="系统自动生成" />
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_RequistHead">
        <div class="am-form-item-title">原仓库名称<font face="宋体">*</font></div>
        <div id="R_StockName" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_RequistHead" >
        <div class="am-form-item-title">原仓库编码</div>
        <input id="R_StockCode" type="text" class="form-control" disabled="disabled" style="background: #f1efef" />
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_RequistHead">
        <div class="am-form-item-title">调拨仓库名称<font face="宋体">*</font></div>
        <div id="R_StockToName" class="form-control" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-6 am-form-item"  data-table="Mes_RequistHead" >
        <div class="am-form-item-title">调拨仓库编码</div>
        <input id="R_StockToCode" type="text" class="form-control" disabled="disabled" style="background: #f1efef" />
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_OutWorkShopHead">
        <div class="am-form-item-title">单据日期<font face="宋体">*</font></div>
        <input id="R_OrderDate" type="text" class="form-control am-input-wdatepicker" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd' })" value="@DateTime.Now.ToString("yyyy-MM-dd")" isvalid="yes" checkexpession="NotNull" disabled="@ViewBag.disabled" />
    </div>
    <div class="col-xs-6 am-form-item" data-table="Mes_RequistHead">
        <div class="am-form-item-title">备注</div>
        <input id="R_Remark" type="text" class="form-control"/>
    </div>
    <div class="col-xs-12 am-form-item" data-table="Mes_RequistHead">
        <div class="am-form-item-title"></div>
        <a id="am_add" class="btn btn-default"><i class="fa fa-plus"></i>&nbsp;添加物料</a>
    </div>
    <div class="col-xs-12 am-form-item am-form-item-grid" >
        <div id="Mes_RequistDetail"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/MesDev/Views/RequistBill/Form.js")
