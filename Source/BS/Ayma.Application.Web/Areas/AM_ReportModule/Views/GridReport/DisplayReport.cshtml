<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <title>显示报表</title>
    <style type="text/css">
        html, body {
            margin: 0;
            height: 100%;
        }
    </style>
    <script src="~/Content/gridreport/CreateControl.js" type="text/javascript"></script>
    <script src="~/Content/gridreport/GRInstall.js" type="text/javascript"></script>
    <script type="text/javascript">
        Install_InsertReport();
        function request(d)
        { for (var c = location.search.slice(1).split("&"), a = 0; a < c.length; a++) { var b = c[a].split("="); if (b[0] == d) if ("undefined" == unescape(b[1])) break; else return unescape(b[1]) } return "" };

        var Report = request('report'), dataURL = request('data');
        if (Report != "") {
            Report = "/Report/grf/" + Report + ".grf";
        }
        if (dataURL != "") {
            dataURL = "/Report/data/DataCenter.ashx?data=" + dataURL;
        }
        if ("@(ViewBag.DataUrl)" != "") {
            dataURL += "@(ViewBag.DataUrl)";
        }
        CreateDisplayViewerEx("100%", "100%", Report, dataURL, true, "");
    </script>
</head>
<body>
    <script type="text/javascript">
        var Installed = Install_Detect();
        if (Installed)
            CreateReport("Report");
    </script>
</body>
</html>